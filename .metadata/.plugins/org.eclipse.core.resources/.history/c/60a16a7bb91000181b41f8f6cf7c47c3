package kivetelkezeles;

public class AlapEset {

	public static void main(String[] args) {
		int szam=0;
		try {
			int[] tomb = {1,2};
			System.out.println(5/szam);
			System.out.println(tomb[2]);
		}catch(Exception e){
			System.out.println("hibatörtént");
			System.out.println(e.getMessage());
			System.out.println(e.getLocalizedMessage());
			e.printStackTrace();// párhuzamosan fut stacktrace, a program fut tovább ezért nem pontosan tt jelenik meg a consolon ahol elõfordult
		}
		System.out.println("program vege");
	}

}
